.container {
  position: absolute;
  top: 0;
  bottom: 44px;
  left: 0;
  right: 0;
}
.dock {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 44px;
  line-height: 44px;
  background: #ff9800;
  font-size: 18px;
  text-align: center;
  color: #fff;
}
.map {
  width: 100%;
  height: 100%;
}

.head {
  height: 44px;
  padding-left: 5%;
  background: #00bcd4;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.select {
  width: 25%;
}

.input {
  width: 60%;
  height: 24px;
  border: solid 1px #666;
}