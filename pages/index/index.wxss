.container {
  position: absolute;
  top: 0;
  bottom: 44px;
  left: 0;
  right: 0;
}
.dock {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 43px;
  line-height: 43px;
  font-size: 18px;
  text-align: center;
  border-top: 1px solid #f5f5f5;
}
.map {
  width: 100%;
  height: 100%;
}
.navigator-item {
  display: inline-block;
  width: 50%;
}
.navigator-item-checked {
  background: #ff9800;
  color: #fff;
}
.navigator-item-normal {
  background: #00bcd4;
  color: #fff;
}